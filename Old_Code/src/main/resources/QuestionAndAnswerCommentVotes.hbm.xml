<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.gobeshonabd.pojos.QuestionAndAnswerCommentVotes" table="question_and_qnswer_comment_votes">
<!--**************** Start Filed Mapping *****************-->
    <!--1. private int  private int questionAndAnswerComment_voteID-->
    <id name="questionAndAnswerComment_voteID" column="question_and_answer_comment_vote_id" >
      <generator class="increment"/>
    </id>
    <!--    2 private int vote_userID -->
    <property name="vote_userID" column="vote_user_id"/>
    <!--    3  vote_QuestionAndAnswerCommentID -->
    <property name="vote_QuestionAndAnswerCommentID" column="vote_question_and_answer_comment_id"/>
    <!--<property name="vote_QuestionAndAnswerID"/>-->
    <!--    4 private String voteDate -->
    <property name="voteDate" column="vote_date"/>
    <!--    5 private int voteValue -->
    <property name="voteValue" column="vote_value"/>
<!--**************** End Filed Mapping *****************-->
  </class>
</hibernate-mapping>
