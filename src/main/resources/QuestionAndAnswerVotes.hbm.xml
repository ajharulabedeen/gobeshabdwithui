<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.gobeshonabd.pojos.QuestionAndAnswerVotes" table="question_and_answer_votes">
    <!--**************** Start Filed Mapping *****************-->
    <!--1. private int articleID-->
    <id name="questionAndAnswer_voteID" column="question_and_answer_vote_id">
      <generator class="increment"/>
    </id>
    <!--    2 private int vote_userID -->
    <property name="vote_userID" column="vote_user_id"/>
    <!--    3 private int vote_articleID -->
    <property name="vote_QuestionAndAnswerID" column="vote_auestion_and_answer_id"/>
    <!--    4 private String voteDate -->
    <property name="voteDate" column="vote_date"/>
    <!--    5 private int voteValue -->
    <property name="voteValue" column="vote_value"/>
    <!--**************** End Filed Mapping *****************-->
  </class>
</hibernate-mapping>
